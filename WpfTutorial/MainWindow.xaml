<Window x:Class="WpfTutorial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTutorial"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" ResizeMode="NoResize" SizeToContent="WidthAndHeight" Topmost="False" WindowState="Normal" Icon="./Resources/git.ico" MouseMove="Window_MouseMove">
    <Window.Resources>
        <sys:String x:Key="strHelloAgain">Hello Again</sys:String>



    </Window.Resources>
    <Canvas Background="Gray" Height="350" Width="525">
        <Label Content="Name" Canvas.Left="30" Canvas.Top="24"/>
        <TextBox Name="UsersName" Height="24" TextWrapping="Wrap" Text="Enter Name" Width="120" Canvas.Left="105" Canvas.Top="26"/>
        <Button Content="Send" Margin="0" Width="75" Canvas.Left="255" Canvas.Top="24" Click="Send_Button_Click"/>

    </Canvas>


</Window>
